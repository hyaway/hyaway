# hyAway Environment Variables
# Local overrides go in .env.local (gitignored)
# Variables prefixed with VITE_ are exposed to the client.

# Base URL for the app (used in og:image, canonical URLs, etc.)
# Self-hosters should set this to their own domain
VITE_APP_URL=https://hyaway.com

# Debug: show swipe zone overlays in review feature
VITE_DEBUG_SWIPE_ZONES=false
