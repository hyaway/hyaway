{
  "files.eol": "\n",
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always" // Or "always"
  },
  "tailwindCSS.classAttributes": ["class", "className", "viewportClassName"],
  "tailwindCSS.classFunctions": [
    "tv",
    "composeTailwindRenderProps",
    "twMerge",
    "cn",
    "cx",
    "clsx"
  ],
  "tailwindCSS.experimental.configFile": "src/styles.css",
  "tailwindCSS.files.exclude": ["**/*.md", ".github/**/*.md"],
  "cSpell.words": [
    "blurhash",
    "BOORU",
    "colours",
    "Hugeicons",
    "hyaway",
    "inboxed",
    "IPFS",
    "nums",
    "overscan",
    "Pannable",
    "partialize",
    "Preconfigured",
    "Renderable",
    "Retryable",
    "Tallymarks",
    "Undelete",
    "viewtime"
  ]
}
