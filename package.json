{
  "name": "hyaway-app",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:host": "vite --port 3000 --host",
    "dev:ts": "tailscale serve --bg 3000",
    "dev:ts:off": "tailscale serve 3000 off",
    "build": "vite build",
    "serve": "vite preview",
    "docker": "docker compose -f docker/docker-compose.build.yml up -d --build",
    "docker:attach": "docker compose -f docker/docker-compose.build.yml up --build",
    "docker:stop": "docker compose -f docker/docker-compose.build.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.build.yml logs -f",
    "docker:pull": "docker compose -f docker/docker-compose.yml up -d",
    "docker:pull:update": "docker compose -f docker/docker-compose.yml pull && docker compose -f docker/docker-compose.yml up -d",
    "docker:dev": "docker compose -f docker/docker-compose.dev.yml up -d",
    "docker:dev:attach": "docker compose -f docker/docker-compose.dev.yml up",
    "docker:ts": "tailscale serve --bg 4929",
    "docker:ts:off": "tailscale serve 4929 off",
    "ts:status": "tailscale serve status",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "check": "prettier --write . && eslint --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "licenses": "pnpx license-checker-rseidelsohn --production --relativeLicensePath --relativeModulePath --out NOTICE --nopeer --excludePrivatePackage --csv"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-devtools": "^0.7.11",
    "@tanstack/react-form": "^1.28.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.158.4",
    "@tanstack/react-router-devtools": "^1.158.4",
    "@tanstack/react-virtual": "^3.13.18",
    "@vidstack/react": "^1.12.13",
    "axios": "^1.13.4",
    "blurhash": "^2.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "lodash.memoize": "^4.1.2",
    "motion": "^12.33.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-zoom-pan-pinch": "^3.7.0",
    "slug": "^11.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vaul-base": "^1.0.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@storybook/react-vite": "^10.2.7",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.4.1",
    "@tanstack/eslint-config": "^0.3.4",
    "@tanstack/eslint-plugin-query": "^5.91.4",
    "@tanstack/router-cli": "^1.158.4",
    "@tanstack/router-plugin": "^1.158.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/lodash.memoize": "^4.1.9",
    "@types/node": "^24.10.12",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/slug": "^5.0.9",
    "@vitejs/plugin-react": "^5.1.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-compat": "^6.1.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-storybook": "10.0.1",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "shadcn": "^3.8.4",
    "storybook": "^10.2.7",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitepress": "^1.6.4",
